 RowBox[{
  RowBox[{"tauToIlk", "[", 
   RowBox[{"tau_", ",", "ptau_", ",", "pilkr_"}], "]"}], ":=", 
  RowBox[{
   FractionBox["ptau", "tau"], "-", "pilkr"}]}], "\[IndentingNewLine]", 
